<Application x:Class="DirectoryPermissionTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:directoryPermissionTool="clr-namespace:DirectoryPermissionTool"
             xmlns:valueConverters="clr-namespace:ValueConverters;assembly=ValueConverters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <directoryPermissionTool:ViewModel x:Key="MainViewModel" />
        <valueConverters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    </Application.Resources>
</Application>